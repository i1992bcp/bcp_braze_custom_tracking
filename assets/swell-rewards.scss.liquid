$min: min-width;
$small: 480px;
$medium: 768px;
$large: 1024px;
$theme-font-montserrat: 'Montserrat',
sans-serif;
$theme-font-mont-regular: 'Montserrat';
$theme-font-roboto: 'Roboto Slab', serif;
@font-face {
  font-family: 'Montserrat';
  font-style: normal;
  font-weight: 400;
  font-display: swap;
  src: local('Montserrat Regular'), local('Montserrat-Regular'), url(https://fonts.gstatic.com/s/montserrat/v14/JTUSjIg1_i6t8kCHKm459Wlhzg.ttf) format('truetype');
}
.swell-loader-spinner {
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  height: 100%;
  img {
    width: 50px !important;
    height: 50px !important;
  }
}
.swell-BCP-rewards {
  *,
  *::before,
  *::after {
    box-sizing: inherit;
  }
  input:-webkit-autofill,
  input:-webkit-autofill:hover,
  input:-webkit-autofill:focus,
  input:-webkit-autofill:active {
    transition: background-color 5000s ease-in-out 0s;
  }
  ul {
    padding: 0;
    margin: 0;
    li {
      list-style-type: none;
    }
  }
  img {
    width: 100%;
    height: 100%;
    -o-object-fit: cover;
    object-fit: cover;
    -o-object-position: center top;
    object-position: center top;
    vertical-align: -webkit-baseline-middle;
  }
  .swell-desktop {
    display: none;
  }
  .swell-mobile {
    display: block;
  }
  @media ($min:$medium) {
    .swell-desktop {
      display: block;
    }
    .swell-mobile {
      display: none;
    }
  }
  .swell-section-heading {
    margin-bottom: 30px;
    @media($min: $small) {
      margin-bottom: 40px;
    }
    @media($min: $medium) {
      margin-bottom: 45px;
    }
    .swell-heading {
      font-family: $theme-font-roboto;
      font-size: 19px;
      font-weight: normal;
      font-stretch: normal;
      font-style: normal;
      line-height: normal;
      letter-spacing: normal;
      text-align: center;
      color: #282828;
      margin-bottom: 20px;
      @media($min: $small) {
        margin-bottom: 25px;
        font-size: 28px;
      }
      @media($min: $medium) {
        margin-bottom: 12px;
        font-size: 27px;
      }
    }
    .swell-detail {
      font-family: $theme-font-mont-regular;
      font-size: 13.5px;
      font-weight: normal;
      font-stretch: normal;
      font-style: normal;
      line-height: normal;
      letter-spacing: normal;
      text-align: center;
      color: #989998;
      margin: 0 auto;
      max-width: 290px;
      @media($min: $small) {
        font-size: 20.5px;
        max-width: 440px;
      }
      @media($min: $medium) {
        font-size: 16px;
        max-width: 100%;
      }
      span {
        font-family: $theme-font-montserrat;
        font-weight: bold;
      }
    }
  }
  .swell-link-list {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    .swell-link {
      margin-bottom: 15px;
      @media($min: $small) {
        margin-bottom: 20px;
      }
      &:last-child {
        margin: 0;
        .swell-link-button {
          text-decoration: underline;
          background: transparent;
          border: 0;
          height: 100%;
          min-width: 100%;
        }
      }
      .swell-link-button {
        display: block;
        cursor: pointer;
        font-family: $theme-font-montserrat;
        font-size: 11px;
        font-weight: 600;
        font-stretch: normal;
        font-style: normal;
        line-height: normal;
        letter-spacing: normal;
        text-align: center;
        color: #ffffff;
        width: 100%;
        margin: 0 auto;
        text-decoration: none;
        border-radius: 4px;
        display: flex;
        align-items: center;
        justify-content: center;
        text-transform: uppercase;
        min-width: 135px;
        height: 36px;
        background-color: #ae8d4f;
        border: 1px solid #ae8d4f;
        @media ($min: $small) {
          min-width: 205px;
          height: 45px;
          font-size: 16px;
        }
        @media($min: $medium) {
          height: 42px;
          min-width: 365px;
          font-size: 15px;
        }
        &:hover {
          background: transparent;
          color: #ae8d4f;
        }
      }
    }
  }
  .swell-container {
    max-width: 1340px;
    margin: 0 auto;
  }
  .swell-banner {
    display: flex;
    justify-content: center;
    align-items: center;
    max-width: 1340px;
    margin: 0 auto;
    @media($min: $medium) {
      padding: 0 20px;
    }
    @media($min: 969px) {
      padding: 0 30px;
    }
    @media($min: 1340px) {
      padding: 0;
    }
    .swell-banner-outer-holder {
      position: relative;
      width: 100%;
      @media($min: $medium) {
        background: transparent;
      }
      .swell-desktop-banner {
        min-height: 450px;
      }
      .banner-holder {
        position: absolute;
        top: 0;
        display: flex;
        align-items: flex-end;
        justify-content: center;
        width: 100%;
        height: 100%;
        border-bottom: 0;
        background: rgba(34, 34, 34, 0.4);
        margin: 0;
        @media($min: $medium) {
          align-items: center;
        }
        .banner-caption {
          text-align: center;
          padding: 15px 0;
          margin: 0 auto;
          @media($min: $small) {
            padding: 20px 0;
          }
          @media ($min: $medium) {
            padding: 130px 0 30px;
            margin: 0 auto;
            text-align: left;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            width: 100%;
          }
          &.logged-in {
            padding: 15px 0 30px;
            @media($min: $small) {
              padding: 20px 0 40px;
            }
            @media ($min: $medium) {
              padding: 95px 0 30px;
            }
            .banner-caption-holder {
              .swell-mobile-logo {
                margin: 0 auto 40px;
                @media($min: $small) {
                  margin: 0px auto 55px;
                }
              }
            }
          }
          .banner-caption-holder {
            margin: 0 auto;
            max-width: 260px;
            @media($min: $small) {
              max-width: 410px;
            }
            @media($min: $medium) {
              margin: 0 auto;
              max-width: 570px;
            }
            .swell-desktop-logo {
              @media(min-width: 768px) {
                margin: 0 auto 85px;
                max-width: 565px;
                min-width: 565px;
              }
            }
            .swell-mobile-logo {
              margin: 0 auto 30px;
              max-width: 185px;
              min-width: 185px;
              @media($min: $small) {
                margin: 0px auto 40px;
                max-width: 275px;
                min-width: 275px;
              }
            }
            .swell-customer-name {
              font-family: $theme-font-roboto;
              font-size: 20px;
              font-weight: normal;
              font-stretch: normal;
              font-style: normal;
              line-height: normal;
              letter-spacing: normal;
              text-align: center;
              color: #ffffff;
              text-transform: capitalize;
              @media($min: $small) {
                font-size: 28px;
              }
              @media($min: $medium) {
                font-size: 32px;
              }
            }
          }
        }
      }
    }
  }
  .swell-hiw-joining {
    max-width: 1340px;
    background: #f9f7f2;
    padding: 40px 0 25px;
    margin: 0 0 30px;
    @media($min: $small) {
      padding: 45px 0 30px;
      margin: 0 0 35px;
    }
    @media($min: $medium) {
      padding: 45px 0 75px;
      margin: 0 20px 65px;
    }
    @media($min: 969px) {
      margin: 0 30px 65px;
    }
    @media($min: 1340px) {
      margin: 0 auto 65px;
    }
    &.logged-in {
      padding: 40px 0;
      @media($min: $small) {
        padding: 45px 0;
      }
      @media($min: $medium) {
        padding: 45px 0 30px;
      }
    }
    .swell-section-heading {
      .swell-heading {
        display: flex;
        align-items: center;
        justify-content: center;
        .swell-joining-logo {
          object-fit: contain;
          max-width: 35px;
          margin-left: 15px;
          @media($min: $small) {
            max-width: 50px;
            margin-left: 20px;
          }
          @media($min: $medium) {
            max-width: 40px;
            margin-left: 20px;
          }
        }
      }
    }
    .swell-joining-section {
      display: flex;
      justify-content: center;
      align-items: center;
      flex-direction: column;
      margin-bottom: 45px;
      @media($min: $small) {
        margin-bottom: 55px;
      }
      @media($min: $medium) {
        flex-direction: row;
        align-items: flex-start;
        margin-bottom: 50px;
      }
      .swell-joining-outer-holder {
        position: relative;
        flex-basis: 33.3%;
        display: flex;
        justify-content: center;
        align-items: center;
        width: 100%;
        padding-bottom: 40px;
        margin-bottom: 50px;
        @media($min: $small) {
          padding-bottom: 45px;
          margin-bottom: 55px;
        }
        @media($min:$medium) {
          padding-bottom: 0;
          margin-bottom: 0;
        }
        &:after {
          position: absolute;
          content: "";
          background-color: #979797;
          width: 100%;
          height: 100%;
          min-height: 1px;
          max-height: 1px;
          bottom: 0;
          max-width: 180px;
          @media($min: $small) {
            max-width: 250px;
          }
          @media($min: $medium) {
            max-width: 1px;
            min-height: 78px;
            max-height: 78px;
            top: 0;
            right: 0;
            bottom: unset;
          }
          @media($min: $large) {
            min-height: 100px;
            max-height: 100px;
          }
        }
        &:nth-child(2) {
          margin-bottom: 30px;
          padding-bottom: 50px;
          @media($min: $small) {
            margin-bottom: 35px;
            padding-bottom: 55px;
          }
          @media($min: $medium) {
            padding-bottom: 0;
            margin-bottom: 0px;
          }
        }
        &:last-child {
          margin-bottom: 0;
          padding-bottom: 0;
          &:after {
            display: none;
          }
        }
        .swell-joining-holder {
          display: flex;
          justify-content: center;
          align-items: center;
          flex-direction: column;
          .swell-joining-heading {
            font-family: $theme-font-montserrat;
            font-size: 14px;
            font-weight: bold;
            font-stretch: normal;
            font-style: normal;
            line-height: normal;
            letter-spacing: normal;
            text-align: center;
            color: #282828;
            text-transform: uppercase;
            margin-bottom: 7px;
            @media($min: $small) {
              font-size: 20px;
              margin-bottom: 10px;
            }
            @media($min:$medium) {
              font-size: 12px;
              margin-bottom: 15px;
            }
            @media($min: $large) {
              font-size: 18px;
            }
          }
          .swell-customer-status {
            font-family: $theme-font-roboto;
            font-size: 24px;
            font-weight: bold;
            font-stretch: normal;
            font-style: normal;
            line-height: normal;
            letter-spacing: normal;
            text-align: center;
            color: #282828;
            text-transform: uppercase;
            @media($min: $small) {
              font-size: 31px;
            }
            @media($min: $medium) {
              font-size: 22px;
              margin-bottom: 5px;
            }
            @media($min:$large) {
              font-size: 32px;
            }
          }
          .swell-start-shopping {
            font-family: $theme-font-montserrat;
            font-size: 11px;
            font-weight: normal;
            font-stretch: normal;
            font-style: italic;
            line-height: normal;
            letter-spacing: normal;
            text-align: center;
            color: #989998;
            margin-top: 7px;
            @media($min: $small) {
              font-size: 15px;
              margin-top: 10px;
            }
            @media($min: $medium) {
              margin-top: 0px;
              font-size: 11px;
            }
            @media($min: $large) {
              font-size: 12px;
            }
          }
        }
      }
    }
    .swell-link-list {
      .swell-link {
        .swell-link-button {
          min-width: 290px;
          height: 50px;
          @media($min: $small) {
            min-width: 430px;
            height: 60px;
          }
          @media($min: $medium) {
            min-width: 365px;
            height: 42px;
          }
          &.swell-account-button {
            background-color: #ae8d4f;
            border: 1px solid #ae8d4f;
            text-decoration: none;
            &:hover {
              background: transparent;
            }
          }
        }
        .swell-link-history {
          color: #282828;
          height: 100%;
          min-width: 100%;
        }
      }
    }
  }
  .swell-campaign-section {
    max-width: 1340px;
    margin: 0 auto 45px;
    @media($min: $small) {
      margin: 0 auto 50px;
    }
    @media($min: $medium) {
      padding: 0 20px;
    }
    @media($min: 969px) {
      padding: 0 30px;
    }
    @media($min: 1340px) {
      padding: 0;
    }
    .swell-section-heading {
      margin-bottom: 55px;
      @media($min: $small) {
        margin-bottom: 65px;
      }
      @media($min: $medium) {
        margin-bottom: 60px;
      }
      .swell-detail {
        font-size: 12px;
        max-width: 280px;
        @media($min: $small) {
          font-size: 18px;
          max-width: 420px;
        }
        @media($min: $medium) {
          max-width: 100%;
          font-size: 16px;
        }
      }
    }
    .swell-campaign-holder {
      padding: 0 12.5px;
      @media($min: $medium) {
        padding: 0;
      }
      .swell-campaign-heading {
        font-family: $theme-font-roboto;
        font-size: 18.5px;
        font-weight: normal;
        font-stretch: normal;
        font-style: normal;
        line-height: normal;
        letter-spacing: normal;
        color: #282828;
        text-align: center;
        margin-bottom: 30px;
        @media($min: $small) {
          font-size: 27px;
          margin-bottom: 40px;
        }
        @media($min: $medium) {
          text-align: left;
          font-size: 21px;
          margin-bottom: 20px;
        }
      }
      .swell-campaign-list-one-time,
      .swell-campaign-list-multiple {
        justify-content: space-between;
        margin: 0 auto;
        display: flex;
        flex-wrap: wrap;
        .swell-campaign {
          position: relative;
          margin: 0;
          display: flex;
          align-items: center;
          justify-content: center;
          background-color: #f9f7f2;
          border-radius: 5px;
          border: 1px solid #f9f7f2;
          flex-basis: 100%;
          margin-bottom: 20px;
          @media($min: $small) {
            margin-bottom: 25px;
          }
          @media($min: $medium) {
            flex-basis: 31.5%;
            margin-bottom: 30px;
          }
          .swell-campaign-content {
            width: 100%;
            padding: 10px 0;
            @media($min: $small) {
              padding: 15px 0;
            }
            @media($min: $large) {
              padding: 20px 0;
            }
            .swell-campaign-image {
              height: 100%;
              margin: 0 auto;
              max-width: 65px;
              margin-bottom: 10px;
              @media($min: $small) {
                max-width: 95px;
                margin-bottom: 15px;
              }
              @media($min:$medium) {
                margin-bottom: 8px;
                max-width: 65px;
              }
              @media($min:$large) {
                margin-bottom: 12px;
                max-width: 90px;
              }
              img {
                object-fit: contain;
              }
            }
            .swell-campaign-value {
              font-family: $theme-font-roboto;
              font-size: 17px;
              font-weight: bold;
              font-stretch: normal;
              font-style: normal;
              line-height: normal;
              letter-spacing: normal;
              text-align: center;
              color: #ae8d4f;
              text-transform: uppercase;
              @media($min: $small) {
                font-size: 27px;
              }
              @media($min: $medium) {
                font-size: 18px;
              }
              @media($min: $large) {
                font-size: 24px;
              }
            }
            .swell-campaign-type {
              text-align: center;
              margin-bottom: 10px;
              @media($min: $small) {
                margin-bottom: 15px;
              }
              @media($min: $medium) {
                margin-bottom: 5px;
              }
              @media($min: $large) {
                margin-bottom: 10px;
              }
              .swell-campaign-type-content {
                font-family: $theme-font-montserrat;
                font-size: 14px;
                font-weight: bold;
                font-stretch: normal;
                font-style: normal;
                line-height: normal;
                letter-spacing: normal;
                text-align: center;
                color: #282828;
                text-transform: uppercase;
                @media($min: $small) {
                  font-size: 20.5px;
                }
                @media($min: $medium) {
                  font-size: 13.5px;
                }
                @media($min: $large) {
                  font-size: 18px;
                }
              }
            }
            .swell-campaign-detail {
              text-align: center;
              margin-top: 10px;
              @media($min: $small) {
                margin-top: 15px;
              }
              @media($min: $medium) {
                margin: 5px auto 0;
                max-width: 220px;
              }
              @media($min: $large) {
                margin: 8px auto 0;
                max-width: 290px;
              }
              .swell-campaign-detail-content {
                display: block;
                font-family: $theme-font-mont-regular;
                font-size: 11.5px;
                font-weight: normal;
                font-stretch: normal;
                font-style: normal;
                line-height: normal;
                letter-spacing: normal;
                text-align: center;
                color: #282828;
                @media($min: $small) {
                  font-size: 18px;
                }
                @media($min: $medium) {
                  font-size: 11px;
                  min-height: 28px;
                }
                @media($min: $large) {
                  font-size: 14.5px;
                  min-height: 36px;
                }
                .swell-share-allow {
                  font-family: $theme-font-montserrat;
                  font-size: 11px;
                  font-style: italic;
                  @media($min: $small) {
                    font-size: 15px;
                  }
                  @media($min: $medium) {
                    font-size: 11px;
                  }
                  @media($min: $large) {
                    font-size: 13px;
                  }
                }
              }
            }
          }
          .swell-campaign-completed {
            display: none;
            position: absolute;
            top: 12px;
            right: 20px;
            @media($min: $small) {
              top: 17px;
              right: 27px;
              max-width: 35px;
              min-width: 35px;
            }
            @media($min: $medium) {
              min-width: unset;
              max-width: 100%;
              right: 15px;
            }
            img {
              object-fit: contain;
            }
          }
        }
      }
      .swell-campaign-list-one-time {
        margin-bottom: 30px;
        @media($min: $small) {
          margin-bottom: 35px;
        }
        @media($min: $medium) {
          margin-bottom: 60px;
        }
      }
    }
  }
  .swell-purchase-review {
    background: #f9f7f2;
    padding: 25px 0 60px;
    margin-bottom: 60px;
    @media($min: $small) {
      padding: 30px 0 65px;
      margin-bottom: 65px;
    }
    @media($min: $medium) {
      padding: 60px 0;
      margin-bottom: 65px;
    }
    .swell-purchase-review-heading-holder {
      margin-bottom: 10px;
      @media($min: $small) {
        margin-bottom: 15px;
      }
      @media($min: $medium) {
        margin-bottom: 35px;
      }
      &.logged-in {
        @media($min: $medium) {
          margin-bottom: 15px;
        }
        .swell-purchase-review-detail-holder {
          @media($min: $medium) {
            display: block;
          }
        }
      }
      .swell-heading {
        font-family: $theme-font-roboto;
        font-size: 18px;
        font-weight: normal;
        font-stretch: normal;
        font-style: normal;
        line-height: normal;
        letter-spacing: normal;
        text-align: center;
        color: #333333;
        margin-bottom: 7px;
        @media($min: $small) {
          font-size: 26.5px;
          margin-bottom: 10px;
        }
        @media($min: $medium) {
          font-size: 25.5px;
          margin-bottom: 20px;
        }
        @media($min: $large) {
          font-size: 32px;
        }
      }
      .swell-purchase-review-detail-holder {
        margin: 0 auto;
        padding: 0 30px;
        @media($min: $small) {
          padding: 0 50px;
        }
        @media($min: $medium) {
          display: none;
          padding: 0 17px;
          max-width: 950px;
        }
        .swell-detail {
          display: block;
          font-family: $theme-font-montserrat;
          font-size: 13.5px;
          font-weight: 600;
          font-stretch: normal;
          font-style: normal;
          line-height: normal;
          letter-spacing: normal;
          text-align: center;
          color: #ffffff;
          background-color: #ae8d4f;
          padding: 10px 0;
          @media($min: $small) {
            font-size: 20px;
            padding: 12px 0;
          }
          @media($min: $medium) {
            font-size: 16px;
          }
        }
      }
    }
    .swell-purchase-review-holder {
      .swell-purchase-review-list {
        padding: 0;
        list-style: none;
        padding: 0 30px;
        display: flex;
        align-items: center;
        justify-content: center;
        flex-direction: column;
        text-align: center;
        @media($min: $small) {
          padding: 0 50px;
        }
        @media ($min: $medium) {
          flex-direction: row;
          max-width: 950px;
          margin: 0 auto;
          padding: 0;
        }
        .swell-purchase-review-list-item {
          width: 100%;
          background-color: #282828;
          border-radius: 5px;
          margin: 0 0 30px;
          padding: 50px 0 30px;
          @media($min: $small) {
            margin: 0 0 35px;
            padding: 55px 0 35px;
          }
          @media ($min: $medium) {
            padding: 35px 0 15px;
            margin: 0 17px;
            flex: 1;
          }
